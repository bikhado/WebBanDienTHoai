                    success: function (msg) {
                        //Lấy phản hồi từ Webserver cho Client
                        username.val(msg.d);
                        $('.loader1').hide(1000);
                    }

                });
                // lấy password
                $.ajax({
                    type: "POST",
                    //Gọi trang và truyền hàm của server
                    url: "Default.aspx/passWord",
                    // truyền các tham số của hàm trong C#.
