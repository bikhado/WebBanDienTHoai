                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (msg) {
                        //Lấy phản hồi từ Webserver cho Client
                        $('#txtPassword1').val(msg.d);
                    }

                });
                $.ajax({
                    type: "POST",
                    //Gọi trang và truyền hàm của server
                    url: "Default.aspx/email",
                    // truyền các tham số của hàm trong C#.
                    //Bạn có thể dung data{DanhSachCacBien}
                    data: "{}",
                    //Nếu không có tham số truyền vào trong hàm bạn có thể dung data: "{}",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (msg) {
                        //Lấy phản hồi từ Webserver cho Client
                        $('#txtEmail1').val(msg.d);
                    }

                });
                $.ajax({
                    type: "POST",
                    //Gọi trang và truyền hàm của server
                    url: "Default.aspx/diaChiGiaoHang",
                    // truyền các tham số của hàm trong C#.
                    //Bạn có thể dung data{DanhSachCacBien}
                    data: "{}",
                    //Nếu không có tham số truyền vào trong hàm bạn có thể dung data: "{}",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (msg) {
                        //Lấy phản hồi từ Webserver cho Client
                        $('#txtDiaChi').val(msg.d);
                    }

                });
                $.ajax({
                    type: "POST",
                    //Gọi trang và truyền hàm của server
                    url: "Default.aspx/SoDienThoai",
                    // truyền các tham số của hàm trong C#.
                    //Bạn có thể dung data{DanhSachCacBien}
                    data: "{}",
                    //Nếu không có tham số truyền vào trong hàm bạn có thể dung data: "{}",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (msg) {
                        //Lấy phản hồi từ Webserver cho Client
                        $('#txtSDT').val(msg.d);
                    }

                });
                $('#thongTinTaiKhoan').modal('show');

            })
            $('#btnThayDoiMatKhau').click(function () {
                if ($('#thaydoimaykhau').is(':visible') == true) {
                    $('#thaydoimaykhau').hide();
                } else {
                    $('#thaydoimaykhau').show();
                }
                

            })
            $('#btnThayDoiEmail').click(function () {
                $('#txtEmail1').attr("readonly", false);
                if ($('#btnThayDoiEmail1').is(':visible') == true) {
                    $('#btnThayDoiEmail1').hide();
                    $('#txtEmail1').attr("readonly", true);
                } else {
                    $('#btnThayDoiEmail1').show();
                    $('#txtEmail1').attr("readonly", false);
                }
            })
            $('#btnThayDoiDC').click(function () {
                $('#txtDiaChi').attr("readonly", false);

            })
            $('#btnUpdatePassword').click(function () {
                var matkhaumoi = $('#txtPasswordMoi').val();
                var matkhaucu = $('#txtPasswordCu').val();
                var matkhaumoinhaplai = $('#txtPasswordMoiNL').val();
                if (matkhaumoi.length < 8) {
                    alert("Mật khẩu phải lớn hơn hoặc bằng 8 ký tự.");
                    return false;
                }
                if (matkhaumoi != matkhaumoinhaplai) {
                    alert("Mật khẩu không khớp.");
                    return false;
                }
                $.ajax({
                    type: "POST",
                    //Gọi trang và truyền hàm của server
                    url: "Default.aspx/updateMatKhau",
                    // truyền các tham số của hàm trong C#.
                    //Bạn có thể dung data{DanhSachCacBien}
                    data: "{pass: '" + matkhaumoi + "',passcu: '" + matkhaucu + "'}",
                    //Nếu không có tham số truyền vào trong hàm bạn có thể dung data: "{}",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (msg) {
                        //Lấy phản hồi từ Webserver cho Client
                        if (msg.d == true) {
                            alert("Cập nhật thành công!");
                            $('#thaydoimaykhau').hide();
                            $('#txtPassword1').val(matkhaumoi);
                        } else alert("Mật khẩu cũ không đúng.!");
                    }

                });
            })
            $('#txtEmail1').keyup(function () {
                var email = $('#txtEmail1').val();

                $.ajax({
                    type: "POST",
                    //Gọi trang và truyền hàm của server
                    url: "register.aspx/checkEmail",
                    // truyền các tham số của hàm trong C#.
                    //Bạn có thể dung data{DanhSachCacBien}
                    data: "{email:'" + email + "'}",
                    //Nếu không có tham số truyền vào trong hàm bạn có thể dung data: "{}",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (msg) {
                        if (msg.d == true) {
                            $('#checkemail11').show();
                        } else {
                            $('#checkemail11').hide();
                        }

                    }

                });
            })
            $('#btnThayDoiEmail1').click(function () {
                var email = $('#txtEmail1').val();
                $.ajax({
                    type: "POST",
                    //Gọi trang và truyền hàm của server
                    url: "Default.aspx/updateEmail",
                    // truyền các tham số của hàm trong C#.
                    //Bạn có thể dung data{DanhSachCacBien}
                    data: "{email: '" + email + "'}",
                    //Nếu không có tham số truyền vào trong hàm bạn có thể dung data: "{}",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (msg) {
                        //Lấy phản hồi từ Webserver cho Client
                        if (msg.d == true) {
                            alert("Cập nhật thành công!");
                            $('#btnThayDoiEmail1').hide();
                            $('#txtEmail1').attr("readonly", true);
                            $('#txtEmail1').val(matkhaumoi);
                        } else alert("Cập nhật thất bại.!");
                    }

                });
            })
        });
    </script>
    
             
