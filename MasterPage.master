    <script>

        $(document).ready(function () {

            $(window).load(function () {
                $('#preloader').fadeOut('slow', function () { $(this).remove(); });
            });
            setTimeout(function () {
                $('#preloader').fadeOut('slow', function () { $(this).remove(); });
            }, 1000);

            $('#myUL').show();
            $('#alogin').click(function () {
                var user1 = $('#welcomeUser');
                if (user1.text() != "")
                {
                    $.ajax({
                        type: "POST",
                        //Gọi trang và truyền hàm của server
                        url: "Default.aspx/checkAdmin",
                        // truyền các tham số của hàm trong C#.
                        //Bạn có thể dung data{DanhSachCacBien}
                        data: "{}",
                        //Nếu không có tham số truyền vào trong hàm bạn có thể dung data: "{}",
                        contentType: "application/json; charset=utf-8",
                        dataType: "json",
                        success: function (msg) {
                            //Lấy phản hồi từ Webserver cho Client
                            if (msg.d == true) {
                                $('#adminn').show();
                            } else {
                                $('#adminn').hide();
                            }

                        }

                    });
                    $('#logout').modal('show');


                } else {
                    
                    $('#login').modal('show')
                }
                
            })
            $('#btnLogin').click(function () {
                
                var check = $('#checkRemember').is(':checked');
                var username = $('#inputEmail').val().trim();
                var password = $('#inputPassword').val().trim();
                //alert(check);

                $.ajax({
                    type: "POST",
                    dataType: "json",
                    contentType: "application/json; charset=utf-8",
                    url: "Default.aspx/kiemTraDangNhap",
                    data: "{username: '" + username + "',password: '" + password + "',checkremember: '" + check + "'}",
                    success: function (Record) {

                        if (Record.d == true) {
                            alert("Đăng nhập thành công!");
                            $('#login').modal('hide');
                            $.ajax({
                                type: "POST",
                                dataType: "json",
                                contentType: "application/json; charset=utf-8",
                                url: "Default.aspx/capNhatGH",
                                data: "{}",
                                success: function (Record) {

                                    if (Record.d == true) {
                                        
                                    }
                                }
                            });
                            //$('#welcomeUser').text(username);
                            //$('#welcomelogin').text(username);
                            window.location.reload();
                            <%--//alert(<%=Session["username"].ToString()%>);--%>
                        }
                        else {
                            alert("Sai tên tài khoản hoặc mật khẩu!");
                        }
                    }
                });
                
            });
            //$('#welcomelogin').hover(function () {
            //    if ($('#welcomelogin').text() != "Login") {
            //        $('#thongtin').css("display", "block");
            //    }
            //})
            $("#welcomelogin").hover(function () {
                if ($('#welcomelogin').text() != "Login")
                $('#thongtin').css("display", "block");
            }, function () {
                $('#thongtin').css("display", "none");
            });
            $('#btnLogout').click(function () {
                $.ajax({
                    type: "POST",
                    dataType: "json",
                    contentType: "application/json; charset=utf-8",
                    url: "Default.aspx/logout",
                    data: "{}",
                    success: function (Record) {

                        if (Record.d == true) {
                            window.location.assign("Default.aspx");
                        }
                    }
                });
            });
            $('#inputEmail').keyup(function (e) {
                if (e.keyCode == 13) {
                    $('#btnLogin').click();
                }
            });
            $('#inputPassword').keyup(function (e) {
                if (e.keyCode == 13) {
                    $('#btnLogin').click();
                }
            });
            

            $('#myInput').keyup(function () {
                var timkiem;
                timkiem = $('#myInput').val();
                $('#myUL').show();
                $.ajax({
                    type: "POST",
                    //Gọi trang và truyền hàm của server
                    url: "Default.aspx/ulli",
                    // truyền các tham số của hàm trong C#.
                    //Bạn có thể dung data{DanhSachCacBien}
                    data: "{tensp:'" + timkiem + "'}",
                    //Nếu không có tham số truyền vào trong hàm bạn có thể dung data: "{}",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",

                    success: function (msg) {

                        //$("#header ul").innerHTML = msg.d;
                        document.getElementById("myUL").innerHTML = msg.d;


                    }

                });
                var input, filter, ul, li, a, i;
                //input = document.getElementById("myInput");
                filter = $('#myInput').val().toUpperCase();
                ul = document.getElementById("myUL");
                li = ul.getElementsByTagName("li");
                for (i = 0; i < li.length; i++) {
                    a = li[i].getElementsByTagName("a")[0];
                    if (a.innerHTML.toUpperCase().indexOf(filter) > -1) {
                        li[i].style.display = "";
                    } else {
                        li[i].style.display = "none";

                    }
                }
                if ($('#myInput').val() == "") {
                    $('#myUL').hide();
                }
            });
            $('#btnthongTinTaiKhoan').click(function () {
                $('.loader1').show(1000);
                $('#txtPassword1').attr("readonly", true);
                $('#txtEmail1').attr("readonly", true);
                $('#txtDiaChi').attr("readonly", true);
                $('#thaydoimaykhau').hide();
                $('#btnThayDoiEmail1').hide();


                var username = $('#txtUsername11');

                

                $('#logout').modal('hide');
                //lấy user name
                $.ajax({
                    type: "POST",
                    //Gọi trang và truyền hàm của server
                    url: "Default.aspx/userName",
